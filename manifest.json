{
  "description": "Make zillow more useable with a BIG MAP! Hides the side bar and top bar.",
  "manifest_version": 2,
  "name": "BIG MAP for Zillow",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/borderify",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.zillow.com/*"],
      "js": ["fullScreen.js"]
    }
  ]
}
